    <!--typedef struct{
	RTS_UI8 NbIdCode;           // Number of ID codes in the module : 1, most of the cases or 2, for TM2 digital with 2 ASIC inside (32 outputs for example)
	RTS_UI8 IdCode1;
	RTS_UI8 IdCode2;            // if NbIdCode = 2
	RTS_UI8 TM2AnalogModuleType;// For TM2 analog modules, the module type is not stored into the IdCode
                                    // but in the read table of the module at offset 7
	RTS_UI8 ImplicitPageIndex;	 // 0-31 or 0-63 (new TM3 ? TBC) : index of the implicit page (0 for most of the cases)
	RTS_UI8 ImplicitReadSize;   // 0-127. Size in bytes of the implicit read to be performed for the module
                                // For digital module it must be the just upper entire number of bytes 
                                // corresponding to the number of inputs (ex 4 inputs => ImplicitReadSize = 1)
                                // For special module, inputs (for example %IW) may be located in this buffer
                                // in this case the offset to find them must be filled (see ImplicitInOffset)
	RTS_UI8 ImplicitReadAddr;   // 0-127. Meaningful only for special module 
                                // => address of the page to implicitely read in the module
                                // Note : a full duplex exchange will be performed if 
                                // (ImplicitReadSize == ImplicitWriteSize) AND (ImplicitReadAddr == ImplicitWriteAddr)
	RTS_I8 ImplicitInOffset;    // Meaningful only for special module, 
                                // -1 if no inputs linked to codesys
                                // The offset where the inputs are located in the implicit read buffer otherwise
	RTS_UI8 ImplicitWriteSize;  // 0-127. Size in bytes of the implicit write to be performed for the module
                                // For digital module it must be the just upper entire number of bytes 
                                // corresponding to the number of outputs (ex 12 outputs => ImplicitWriteSize = 2)
                                // For special module, outputs (for example %QW) may be located in this buffer
                                // in this case the offset to find them must be filled (see ImplicitOutOffset)
	RTS_UI8 ImplicitWriteAddr;  // 0-127. Meaningful only for special module 
                                // => address of the page to implicitely write in the module
                                // Note : a full duplex exchange will be performed if 
                                // (ImplicitReadSize == ImplicitWriteSize) AND (ImplicitReadAddr == ImplicitWriteAddr)
	RTS_I8 ImplicitOutOffset;   // Meaningful only for special module, 
                                // -1 if no outputs linked to codesys
                                // The offset where the outputs are located in the implicit write buffer otherwise
	RTS_UI8 ImplicitReadSize2;  // if NbIdCode = 2, only for TM2 digital modules with 2 ASIC
	RTS_UI8 ImplicitWriteSize2; // if NbIdCode = 2, only for TM2 digital modules with 2 ASIC
	RTS_UI8 StatusInImplicit;   // 0 or 1. Meaningful only for special module
								// if 1 the status are included in the ImplicitRead
								// if 0 an explicit read is done on user demand (see StatusAddr)
	RTS_UI8 StatusPageIndex;	// 0-31 or 0-63 (new TM3 ? TBC). Meaningful only for special module
								//index of the status page (0 for most of the cases) => meaningful only if StatusInImplicit == 0
	RTS_UI8 StatusSize;	        // 0-127. Meaningful only for special module
								// Size in bytes of the status 
	RTS_UI8 StatusAddr;	        // 0-127. Meaningful only for special module
								// if StatusInImplicit == 1 : offset of the status in the implicit read buffer 
								// if StatusInImplicit == 0 : offset of the status in the module read memory mapping 
	RTS_UI8 ConfPageIndex;	     // 0-31 or 0-63 (new TM3 ? TBC) : index of the configuration page (0 for most of the cases)
	RTS_UI8 ConfForSpecialModuleSize;
	RTS_UI8 ConfForSpecialModuleAddr;
	RTS_I8 InterByteDelayDC2;   // Meaningful only for TM2 special modules
                                // TM3 does not use this value (fixed reduced delays) => MUST put -1 for those modules !
    }KPI_DEVDESC_DEFINITION;-->
