	<add key="ExecLoaderFileName" value="ExecLoaderWizard.exe"/>
	<!-- Folder containing Firmware files-->
	<add key="FirmwarePath" value="Firmwares &amp; PostConfiguration"/>
	<!-- Folder containing Example Projects-->
	<add key="SampleProjectsPath" value="Examples"/>
