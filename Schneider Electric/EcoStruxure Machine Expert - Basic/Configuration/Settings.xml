	<add key="Timeout.umasGetCommInfo" value="3000"/>
	<add key="Timeout.umasGetDeviceInfo" value="3000"/>
	<add key="Timeout.umasGetDeviceStatus" value="3000"/>
	<add key="Timeout.umasGetReservation" value="3000"/>
	<add key="Timeout.umasInitialize" value="3000"/>
