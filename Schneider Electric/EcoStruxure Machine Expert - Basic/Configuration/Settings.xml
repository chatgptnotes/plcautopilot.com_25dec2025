<?xml version="1.0" encoding="utf-8"?>
<appSettings>
	<add key="SMB" value="set"/>
	<add key="Application.Version" value="1.4"/>
	<add key="Application.Release" value=""/>
	<!-- log4net -->
	<add key="log4net.Config" value=".\Configuration\logger.xml"/>
	<add key="log4net.Config.Watch" value="True"/>
